exports.apply_to_img_coll = function(name_of_img_function,img_coll){
  
  var repo_name = ['users/emanuelespiritowork/SharedRepo:functions/',name_of_img_function];
  repo_name = repo_name.join("");
  print(repo_name);
  var repo_function = require(repo_name);
  
  return img_coll.map(repo_function.repo_function);
};