
exports.landsat_mask_img = function(image){
  var qa_pixel_layer = image.select("QA_PIXEL");
  
  var dilated_clouds = 1 << 1;
  var cirrus = 1 << 2;
  var cloud = 1 << 3;
  var cloud_shadow = 1 << 4;
  var snow = 1 << 5;
  
  var dilated_clouds_mask = qa_pixel_layer
  .bitwiseAnd(dilated_clouds);
  
};